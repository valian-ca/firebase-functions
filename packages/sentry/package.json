{
  "name": "@valian/node-sentry",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valian-ca/firebase-functions.git"
  },
  "author": {
    "name": "Valian",
    "organization": true,
    "url": "https://valian.ca"
  },
  "contributors": [
    {
      "name": "Julien Marcil",
      "email": "julien@valian.ca"
    }
  ],
  "keywords": [
    "firebase"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@sentry/core": "^10",
    "@sentry/node": "^10",
    "firebase-functions": "^6 || ^7"
  },
  "dependencies": {
    "@valian/function-logger": "workspace:*"
  },
  "devDependencies": {
    "@sentry/core": "10.25.0",
    "@sentry/node": "10.27.0",
    "firebase-functions": "7.0.0"
  },
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": {
          "import": "./lib/index.d.mts",
          "require": "./lib/index.d.cts"
        },
        "import": "./lib/index.mjs",
        "require": "./lib/index.cjs",
        "default": "./lib/index.mjs"
      },
      "./init": {
        "types": {
          "import": "./lib/init-sentry.d.mts",
          "require": "./lib/init-sentry.d.cts"
        },
        "import": "./lib/init-sentry.mjs",
        "require": "./lib/init-sentry.cjs",
        "default": "./lib/init-sentry.mjs"
      }
    },
    "main": "./lib/index.cjs",
    "module": "./lib/index.mjs",
    "types": "./lib/index.d.mts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./init": {
      "types": "./src/init-sentry.ts",
      "import": "./src/init-sentry.ts",
      "default": "./src/init-sentry.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "prepack": "pnpm run build",
    "lint:publint": "pnpm build && publint"
  },
  "engines": {
    "node": ">=22.18"
  }
}
